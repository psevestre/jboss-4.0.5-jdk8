<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 6.0.0 EJB RDBMS Persistence//EN" "http://www.bea.com/servers/wls600/dtd/weblogic-rdbms20-persistence-600.dtd">

<weblogic-rdbms-jar>
   <weblogic-rdbms-bean>
      <ejb-name>Secret</ejb-name>
      <data-source-name>java:/DefaultDS</data-source-name>
      <table-name>secret</table-name>
      <field-map>
         <cmp-field>secretKey</cmp-field>
         <dbms-column>secret_key</dbms-column>
      </field-map>
      <field-map>
         <cmp-field>secret</cmp-field>
         <dbms-column>secret</dbms-column>
      </field-map>

   </weblogic-rdbms-bean>

   <create-default-dbms-tables>True</create-default-dbms-tables>

</weblogic-rdbms-jar>
