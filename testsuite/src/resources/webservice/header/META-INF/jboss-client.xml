<?xml version='1.0' encoding='UTF-8' ?>

<!DOCTYPE jboss-client PUBLIC
   "-//JBoss//DTD Application Client 4.0//EN"
   "http://www.jboss.org/j2ee/dtd/jboss-client_4_0.dtd">

<jboss-client>
  <jndi-name>ws4ee-client</jndi-name>

  <service-ref>
    <service-ref-name>service/SimpleHeaderService</service-ref-name>
    <wsdl-override>http://@jbosstest.host.name@:8080/ws4ee-header/simple?wsdl</wsdl-override>
  </service-ref>

  <service-ref>
    <service-ref-name>service/BeanHeaderService</service-ref-name>
    <wsdl-override>http://@jbosstest.host.name@:8080/ws4ee-header/bean?wsdl</wsdl-override>
  </service-ref>

  <service-ref>
    <service-ref-name>service/ImplicitHeaderService</service-ref-name>
    <wsdl-override>http://@jbosstest.host.name@:8080/ws4ee-header/implicit?wsdl</wsdl-override>
  </service-ref>

  <service-ref>
    <service-ref-name>service/ExplicitHeaderService</service-ref-name>
    <wsdl-override>http://@jbosstest.host.name@:8080/ws4ee-header/explicit?wsdl</wsdl-override>
  </service-ref>

</jboss-client>
